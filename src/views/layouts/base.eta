<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= it.title %></title>
    <% const css = it.css || ["/css/style.css"] %>
    <% css.forEach((path) => { %>
        <link rel="stylesheet" href="<%= path %>">
    <% }) %>
</head>
<body>
    <%~ it.body %>
</body>
</html>
