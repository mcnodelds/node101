<img src="<%= it.pfplink ? it.pfplink : 'https://github.com/'.concat(it.gh).concat(".png") %>" class="devteam-avatar" />
<div class="devteam-member-info">
    <div class="devteam-member-name">
        <a href="<%= "https://github.com/".concat(it.gh) %>">
            <%= it.name %>
        </a>
    </div>
    <div class="devteam-member-role">
        <%= it.role %>
    </div>
    <div class="devteam-member-desc">
        <%= it.description %>
    </div>
</div>
